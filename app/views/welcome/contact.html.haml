- title "Commodity Live Contact"

%main#app-main.app-main
  .container
    .wrap.m-b-lg
      %section.app-content
        .row
          .col-md-10.col-md-offset-1
            .text-center
              %h2.heading Contact
              %p.m-b-xl Do you have questions, feedback or need support? We are here to help you!
        .contact-us
          .row
            .col-sm-8.col-sm-push-2.col-md-6.col-md-push-3
              %form
                %input.form-control.input-lg{:placeholder => "Your Name", :type => "text"}/
                %input.form-control.input-lg{:placeholder => "Your Email", :type => "text"}/
                %textarea.form-control.input-lg{:placeholder => "Your Message"}
                %button.btn.btn-primary.btn-lg{:type => "submit"} SUBMIT
              %ul.list-unstyled
                %li NTTY AS
                %li Grensen 12
                %li 0159 Oslo
                %li info@ntty.com
                %li
                  %a{:href => "http://ntty.com", :target => "_blank"} www.ntty.com
            / col
          / row
      / .app-content
    / .wrap
